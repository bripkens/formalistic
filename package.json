    "url": "http://github.com/bripkens"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bripkens/formalistic/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/eslint-parser": "^7.14.7",
    "babel-jest": "^27.0.6",
    "@babel/preset-env": "^7.14.8",
    "coveralls": "^3.1.1",
    "eslint": "^7.31.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6"
  }
