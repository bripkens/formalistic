    "test:unit": "jest --coverage --collectCoverageFrom=\"src/**/*.js\"",
    "test:lint": "eslint src/**/*.js test/**/*.js",
    "test:watch": "jest --coverage --collectCoverageFrom=\"src/**/*.js\" --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/bripkens/formalistic.git"
  },
  "keywords": [
