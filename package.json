  "description": "Handle form data and form validation",
  "main": "lib/index.js",
  "types": "formalistic.d.ts",
  "files": [
    "src",
    "lib",
    "formalistic.d.ts"
  ],
  "scripts": {
    "prepare": "husky install",
    "build": "rm -rf lib/* && babel src -d lib",
